{
  "name": "supercalifragilistic-run",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --fix",
    "test:unit": "jest test/unit",
    "test:unit:watch": "jest  --watchAll test/unit",
    "test:bdd": "jest --verbose test/bdd/features",
    "test:smoke": "jest --verbose test/bdd/features/smoke.feature",
    "test:smoke-tag": "TAGS=smoke jest --verbose test/bdd/features",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/pwa": "^3.3.5",
    "@vue-hero-icons/outline": "^1.7.2",
    "core-js": "^3.8.3",
    "nuxt": "^2.15.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0",
    "@nuxt/test-utils": "^0.2.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.0.3",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cucumber-jest": "^0.2.4",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "push-dir": "^0.4.1",
    "vue-jest": "^3.0.4"
  }
}
